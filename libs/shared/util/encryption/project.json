{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "libs/shared/util/encryption/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/util/encryption",
        "main": "libs/shared/util/encryption/src/index.ts",
        "tsConfig": "libs/shared/util/encryption/tsconfig.lib.json",
        "assets": ["libs/shared/util/encryption/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/util/encryption/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/util/encryption"],
      "options": {
        "jestConfig": "libs/shared/util/encryption/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
